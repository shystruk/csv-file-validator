    "test": "nyc ava --browser && npm run coverage",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "keywords": [
    "csv parser",
    "parser",
    "validator",
    "csv validator",
    "csv file validator",
    "reviewer",
    "csv reviewer"
  ],
  "author": {
    "name": "Vasyl Stokolosa",
    "email": "v.stokol@gmail.com",
    "url": "https://github.com/shystruk"
  },
  "license": "MIT",
  "dependencies": {
    "famulus": "2.1.2",
    "lodash": "4.17.15",
    "papaparse": "^5.2.0",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "codecov.io": "^0.1.6",
    "nyc": "^11.4.1"
  },
  "snyk": true
}
